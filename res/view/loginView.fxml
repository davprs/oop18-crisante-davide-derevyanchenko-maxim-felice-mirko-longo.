            	<Font name="System Italic" size="13.0" />
         	</font></Button>
      	<Button fx:id="regBtn" mnemonicParsing="false" onAction="#register" text="Register" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
        	<font>
            	<Font size="13.0" />
         	</font></Button>
      	<Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exit" text="Exit" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
